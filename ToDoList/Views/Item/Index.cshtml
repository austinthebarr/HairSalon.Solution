<!DOCTYPE html>
<html>
  <head>
    <title>To DO List!</title>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
  </head>
  <h1>To Do List</h1>
  <table class="table table-hover table-danger">
  <thead>
    <tr>
      <th scope="col">Description</th>
      <th scope="col">DueDate</th>
      <th scope="col">Category</th>
    </tr>
  </thead>
  <tbody>

  @foreach(var items in Model)
  {
<tr>
      <td>@items.GetDescription() | <a href="/items/@items.GetId()/update">Edit</a> | <a href="/items/@items.GetId()/delete">Delete</a>
      </td>
      <td>@items.GetDueDate()</td>
      <td>@items.GetCategoryId()</td>
</tr>
  }

  </tbody>
</table>

<ul>
<!-- @foreach(var items in Model)
{
  <li>@items.GetDescription() is due @items.GetDueDate()</li>
}
</ul> -->
<a href="/items/new" class="btn btn-success">Add a new item.</a><br>
</html>
