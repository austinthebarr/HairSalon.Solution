<!DOCTYPE html>
<html>
<head>
  <title>Hair Salon</title>
  <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>\
  <link rel="stylesheet" type="text/css" href="~/css/clientpage.css" />
  <link href="https://fonts.googleapis.com/css?family=Knewave|Quicksand|Shadows+Into+Light" rel="stylesheet">
</head>
<div class="jumbotron">
  <h1>Specialties</h1>
</div>
<div class="clients">
@foreach (var specialties in Model)
{
  <div class="card" style="width: 18rem; background-color:#b3c2d8">
    <div class="card-body">
      <h5 class="card-title">@specialties.GetDescription()</h5>
      <a href="/stylist/@specialties.GetId()/info" class="card-link">Info/Add</a>
    </div>
  </div>
}
</div>
<a href="/">Back to Home</a><br>
</html>
