<!DOCTYPE html>
<html>
<head>
  <title>Hair Salon</title>
  <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
</head>
<div class="jumbotron">
  <h1>Clients</h1>
</div>
@foreach (var clients in Model)
{
  <div class="card" style="width: 18rem; background-color:#b3c2d8">
    <div class="card-body">
      <h5 class="card-title">@clients.GetName()</h5>
    </div>
    <a href="/stylist/index">Stylists</a>
  </div>
}

</html>
