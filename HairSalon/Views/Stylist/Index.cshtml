<!DOCTYPE html>
<html>
<head>
  <title>Hair Salon</title>
  <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>\
  <link rel="stylesheet" type="text/css" href="~/css/clientpage.css" />
  <link href="https://fonts.googleapis.com/css?family=Knewave|Quicksand|Shadows+Into+Light" rel="stylesheet">
</head>
<div class="jumbotron">
  <h1>Stylists</h1>
</div>
<div class="clients">
@foreach (var stylists in Model)
{
  <div class="card" style="width: 18rem; background-color:#b3c2d8">
    <div class="card-body">
      <h5 class="card-title">@stylists.GetName()</h5>
      <a href="/stylist/@stylists.GetId()/info" class="card-link">Info/Edit</a> ||
      <a href="/stylist/@stylists.GetId()/clients" class="card-link">Clients</a> ||
      <a href="/stylist/@stylists.GetId()/delete">Remove Stylist</a> ||
      <a href="/stylist/@stylists.GetId()/specialty">Add/View Specialty</a>
    </div>
  </div>
}
</div>
<a href="/">Back to Home</a><br>
<a href="/stylist/deleteAll">Delete All Stylists</a>
</html>
